[user]
	name = shadowbq
	email = shadowbq@gmail.com
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[github]
	user = shadowbq
	email = shadowbq@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/bin/vim
	ignorecase = false
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch  
	df = diff
	tree = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
	staged = diff --cached
	diff-staged = diff --cached
	unstage = reset HEAD --
	ignored = ls-files --others
	oneline = log --oneline --decorate
	last = log --oneline -1 HEAD
	out = log --oneline origin/master..HEAD
	wdiff = diff --color-words
	info = remote -v
	unstash = stash pop
	remove-tags = "!f() { git tag -l ${1} | xargs -I % echo \"git tag -d % && git push --delete origin %\" | sh; }; f"
	squash-all = "!f(){ git reset $(git commit-tree HEAD^{tree} -m \"${1:-A New Hope.}\");};f"
	markdown-toc = "git-md-toc-creator"
[http]
	sslVerify = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
        tool = sourcetree
[merge]
        tool = sourcetree
[difftool "sourcetree"]
  cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
  trustExitCode = true
[hub]
  protocol = https
